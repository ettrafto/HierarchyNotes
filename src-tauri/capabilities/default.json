{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for main and note windows",
  "windows": ["main", "note-*"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "core:window:allow-outer-position",
    "core:window:allow-outer-size"
  ]
}
